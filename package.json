{
  "name": "pajax",
  "description": "Promise based ajax/fetch library",
  "version": "0.4.3",
  "keywords": [
    "ajax",
    "fetch",
    "xhr",
    "promise"
  ],
  "main": "dist/pajax.js",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.de",
    "url": "www.n-fuse.de"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/n-fuse/pajax"
  },
  "scripts": {
    "pretest": "./server/start.sh",
    "test": "testem ci || true",
    "posttest": "./server/stop.sh",
    "build": "jspm build lib/main.js dist/pajax.js --format umd --global-name Pajax --skip-source-maps"
  },
  "devDependencies": {
    "body-parser": "^1.12.3",
    "express": "^4.12.3",
    "jspm": "^0.17.0-beta.7",
    "phantomjs-polyfill": "0.0.1",
    "promise-polyfill": "^3.1.0",
    "testem": "^1.0.0"
  },
  "jspm": {
    "main": "lib/main.js",
    "format": "es6",
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.5"
    }
  }
}
